#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Webgit/web-monorepo1/node_modules/.pnpm/vitepress-theme-demoblock@3.0.7_@algolia+client-search@5.12.0_@types+node@22.8.2_@types+react_3en73pea7hzwvextq5c4ahnny4/node_modules/vitepress-theme-demoblock/bin/node_modules:/mnt/d/Webgit/web-monorepo1/node_modules/.pnpm/vitepress-theme-demoblock@3.0.7_@algolia+client-search@5.12.0_@types+node@22.8.2_@types+react_3en73pea7hzwvextq5c4ahnny4/node_modules/vitepress-theme-demoblock/node_modules:/mnt/d/Webgit/web-monorepo1/node_modules/.pnpm/vitepress-theme-demoblock@3.0.7_@algolia+client-search@5.12.0_@types+node@22.8.2_@types+react_3en73pea7hzwvextq5c4ahnny4/node_modules:/mnt/d/Webgit/web-monorepo1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Webgit/web-monorepo1/node_modules/.pnpm/vitepress-theme-demoblock@3.0.7_@algolia+client-search@5.12.0_@types+node@22.8.2_@types+react_3en73pea7hzwvextq5c4ahnny4/node_modules/vitepress-theme-demoblock/bin/node_modules:/mnt/d/Webgit/web-monorepo1/node_modules/.pnpm/vitepress-theme-demoblock@3.0.7_@algolia+client-search@5.12.0_@types+node@22.8.2_@types+react_3en73pea7hzwvextq5c4ahnny4/node_modules/vitepress-theme-demoblock/node_modules:/mnt/d/Webgit/web-monorepo1/node_modules/.pnpm/vitepress-theme-demoblock@3.0.7_@algolia+client-search@5.12.0_@types+node@22.8.2_@types+react_3en73pea7hzwvextq5c4ahnny4/node_modules:/mnt/d/Webgit/web-monorepo1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress-register-components.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress-register-components.js" "$@"
fi
